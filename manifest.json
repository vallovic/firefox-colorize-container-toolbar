{
	"manifest_version": 2,
	"name": "Colorize Container Toolbar - Light & Dark",
	"version": "1.0.3",
	"description": "Colorize your browser toolbar by matching the color of the active container tab",
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
		"contextualIdentities",
		"cookies",
		"tabs",
		"theme"
	],
	"browser_specific_settings": {
		"gecko": {
			"strict_min_version": "89.0"
		}
	},
	"icons": {
		"48": "icons/icon-48.png",
		"96": "icons/icon-96.png",
		"256": "icons/icon.png"
	}
}
